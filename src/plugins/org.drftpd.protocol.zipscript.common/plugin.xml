<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="org.drftpd.protocol.zipscript.common" version="3.2.0">
	<doc>
		<doc-text>(Zipscript) - This plugin provides Protocol Extensions for Zipscript on both the Master and Slave Side</doc-text>
	</doc>

	<attributes>
		<attribute id="DenyUnload" value="true" />
	</attributes>
	<requires>
		<import plugin-id="common" exported="true" />
	</requires>
	<runtime>
		<library id="org.drftpd.protocol.zipscript.common" path="classes/" type="code">
			<export prefix="*" />
		</library>
		<library id="jackson-core" path="../../../lib/jackson-core-2.9.2.jar" type="code">
			<export prefix="*" />
		</library>
		<library id="jackson-annotations" path="../../../lib/jackson-annotations-2.9.2.jar" type="code">
			<export prefix="*" />
		</library>
		<library id="jackson-databind" path="../../../lib/jackson-databind-2.9.2.jar" type="code">
			<export prefix="*" />
		</library>

	</runtime>
</plugin>
